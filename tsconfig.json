{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/data/projectList.js", "src/app/components/ContactLink.jsx", "src/app/components/ContactButton.jsx", "src/app/components/Footer.jsx", "src/app/components/Heading.jsx", "src/app/components/Project.jsx", "src/app/components/ProjSolo.jsx", "src/app/Projects/ALL/page.jsx", "src/app/Projects/CreativeGrief/page.jsx", "src/app/Projects/K9/page.jsx", "src/app/Projects/KartFest/page.jsx", "src/app/Projects/Perihelia/page.jsx", "src/app/Projects/TMA/page.jsx", "src/app/Contact/page.jsx", "src/app/layout.tsx", "src/app/page.jsx", "src/app/Projects/page.jsx"],
  "exclude": ["node_modules"]
}
